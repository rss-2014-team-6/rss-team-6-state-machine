<launch>
  <include file="$(find rosjava_pkg)/state_machine/launch/config.netbook.launch" />
  
  <node name="rep" pkg="topic_tools" type="relay" args="/camera/image_raw /rss/video" machine="gurtej"/>

  <node name="view" pkg="image_view" type="image_view" args="image:=/rss/video" machine="gurtej" />
  <node name="gui" pkg="rosjava_pkg" type="state_machine-execute" args="StateMachine.StateMachine" machine="gurtej" output="screen" />
  <node name="mtn" pkg="rosjava_pkg" type="motion-execute" args="motion.Listener" machine="gurtej" />
  <node name="sense" pkg="rosjava_pkg" type="sensing-execute" args="Publisher" machine="gurtej" />
  <node name="odo" pkg="rosjava_pkg" type="odometry-execute" args="odometry.Odometry" machine="gurtej" />
  <node name="path" pkg="rosjava_pkg" type="path_planning-execute" args="path_planning.PlannerNode" machine="gurtej" />
  <node name="loc" pkg="rosjava_pkg" type="localization-execute" args="localization.Localization" machine="gurtej">
    <param name="mapFileName" value="($find rosjava_pkg)/maps/global-nav-maze-2011-basic.map" />
  </node>
  <node name="vid" pkg="gscam" type="gscam" machine="gurtej">
    <param name="gscam_config" value="v4l2src device=/dev/video0 ! video/x-raw-rgb, width=160, height=120 ! ffmpegcolorspace" />
  </node>
</launch>
